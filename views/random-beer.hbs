{{#if randomBeer}}
    <h1>Your random beer</h1>

   {{#each randomBeer}}
        <img src="{{image_url}}">
        <h1>{{name}}</h1>
        <h5>{{description}}</h5>
        <p>{{tagline}}</p>
        {{#each food_pairing}}
            <li>{{this}}</li>
        {{/each}}
        <h5>{{brewers_tips}}</h5>
   {{/each}}

{{/if}}